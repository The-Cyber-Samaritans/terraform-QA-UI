# Example Terraform variables file
# Copy this to terraform.tfvars and fill in your values

aws_region           = "us-east-1"
ecr_repository_name  = "intelfoundry-qa-ui"

# Get this from your AWS account - the GitHub OIDC provider ARN
# You may need to create this if it doesn't exist
github_oidc_provider_arn = "arn:aws:iam::YOUR_ACCOUNT_ID:oidc-provider/token.actions.githubusercontent.com"

# Optional: AWS principals that can push/pull from ECR
allowed_aws_principals = [
  # "arn:aws:iam::YOUR_ACCOUNT_ID:role/your-role-name"
]
